}

/* Hero Section */
.hero {
    padding-top: 120px;
    padding-bottom: 80px;
    text-align: center;
    background: linear-gradient(to bottom, #fff 0%, var(--apple-light-gray) 100%);
}

.hero-container {
    max-width: 980px;
