}

/* Cards */
.cards-grid {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
    gap: 30px;
    margin-bottom: 80px;
}

.card {
    background: var(--apple-light-gray);
    border-radius: 18px;
    padding: 40px 30px;
    transition: all 0.3s ease;
    position: relative;
    overflow: hidden;
}

.card::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    height: 4px;
    background: var(--apple-gradient);
    transform: translateY(-100%);
    transition: transform 0.3s ease;
}

.card:hover {
    transform: translateY(-4px);
    box-shadow: var(--apple-shadow);
}

.card:hover::before {
    transform: translateY(0);
}

.card-number {
    font-size: 48px;
    font-weight: 600;
    color: var(--apple-gray);
    opacity: 0.3;
    margin-bottom: 16px;
}

.card-title {
    font-size: 24px;
    font-weight: 600;
    margin-bottom: 16px;
    color: var(--apple-black);
}

.card-text {
    font-size: 17px;
    line-height: 1.47059;
    color: var(--apple-gray);
}

/* Program Accordion */
.accordion {
    max-width: 800px;
    margin: 0 auto;
}

.accordion-item {
    border-bottom: 1px solid #d2d2d7;
    padding: 24px 0;
}

.accordion-header {
    display: flex;
    justify-content: space-between;
    align-items: center;
    cursor: pointer;
    padding: 16px 0;
    transition: all 0.3s ease;
}

.accordion-header:hover {
    color: var(--apple-blue);
}

.accordion-title {
    font-size: 24px;
    font-weight: 500;
    letter-spacing: 0.009em;
}

.accordion-icon {
    font-size: 20px;
    color: var(--apple-gray);
    transition: transform 0.3s ease;
}

.accordion-item.active .accordion-icon {
    transform: rotate(45deg);
}

.accordion-content {
    max-height: 0;
    overflow: hidden;
    transition: max-height 0.3s ease;
}

.accordion-item.active .accordion-content {
    max-height: 1000px;
    padding-bottom: 24px;
}

.accordion-body {
    padding-top: 16px;
    color: var(--apple-gray);
    line-height: 1.58824;
}

/* Feature Cards */
.feature-cards {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));
    gap: 20px;
    margin-top: 60px;
}

.feature-card {
    text-align: center;
    padding: 40px 20px;
    background: white;
    border-radius: 18px;
    box-shadow: 0 2px 8px rgba(0, 0, 0, 0.04);
    transition: all 0.3s ease;
}

.feature-card:hover {
    transform: translateY(-8px);
    box-shadow: 0 12px 24px rgba(0, 0, 0, 0.08);
}

.feature-icon {
    font-size: 48px;
    margin-bottom: 20px;
}

.feature-title {
    font-size: 21px;
    font-weight: 600;
    margin-bottom: 12px;
}

.feature-text {
    color: var(--apple-gray);
    font-size: 17px;
    line-height: 1.47059;
}

/* Prompts Section */
.prompts-section {
    background: var(--apple-light-gray);
    border-radius: 18px;
    padding: 40px;
    margin-top: 40px;
}

.prompt-box {
    background: white;
    border-radius: 12px;
    padding: 24px;
    margin-bottom: 20px;
    font-family: 'SF Mono', Monaco, 'Courier New', monospace;
    font-size: 14px;
    line-height: 1.5;
    color: var(--apple-blue);
    box-shadow: 0 2px 8px rgba(0, 0, 0, 0.04);
    transition: all 0.3s ease;
}

.prompt-box:hover {
    transform: translateX(8px);
    box-shadow: 0 4px 16px rgba(0, 0, 0, 0.08);
}

/* CTA Section */
.cta-section {
    background: var(--apple-gradient);
    color: white;
    text-align: center;
    padding: 100px 0;
    position: relative;
    overflow: hidden;
}

.cta-section::before {
    content: '';
    position: absolute;
    top: -50%;
    left: -50%;
    width: 200%;
    height: 200%;
    background: radial-gradient(circle, rgba(255, 255, 255, 0.1) 0%, transparent 70%);
    animation: pulse 15s ease-in-out infinite;
}

@keyframes pulse {
    0%, 100% { transform: scale(1); opacity: 0.3; }
    50% { transform: scale(1.2); opacity: 0.1; }
}

.cta-content {
    position: relative;
    z-index: 1;
}

.cta-headline {
    font-size: 48px;
    font-weight: 600;
    margin-bottom: 24px;
}

.cta-subheadline {
    font-size: 24px;
