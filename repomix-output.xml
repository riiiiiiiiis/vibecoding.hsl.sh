}

/* Form */
.form-container {
    max-width: 500px;
    margin: 0 auto;
}

.form-group {
    margin-bottom: 20px;
}

.form-input {
    width: 100%;
    padding: 16px 24px;
    border: none;
    border-radius: 12px;
    font-size: 17px;
    background: rgba(255, 255, 255, 0.9);
    color: var(--apple-black);
    transition: all 0.3s ease;
}

.form-input:focus {
    outline: none;
    background: white;
    box-shadow: 0 0 0 4px rgba(255, 255, 255, 0.3);
}

.form-submit {
    width: 100%;
    padding: 16px 32px;
    border: none;
    border-radius: 12px;
    font-size: 17px;
    font-weight: 500;
    background: var(--apple-black);
    color: white;
    cursor: pointer;
    transition: all 0.3s ease;
}

.form-submit:hover {
    transform: translateY(-2px);
    box-shadow: 0 8px 24px rgba(0, 0, 0, 0.3);
}

/* Footer */
.footer {
    background: var(--apple-light-gray);
    padding: 60px 0 40px;
    font-size: 14px;
}

.footer-content {
    max-width: 980px;
    margin: 0 auto;
    padding: 0 22px;
}

.footer-grid {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
    gap: 40px;
    margin-bottom: 40px;
}

.footer-column h5 {
    font-size: 14px;
    font-weight: 600;
    margin-bottom: 16px;
    color: var(--apple-black);
}

.footer-links {
    list-style: none;
}

.footer-links li {
    margin-bottom: 12px;
}

.footer-links a {
    color: var(--apple-gray);
    text-decoration: none;
    transition: color 0.3s ease;
}

.footer-links a:hover {
    color: var(--apple-blue);
}

.footer-bottom {
    padding-top: 40px;
    border-top: 1px solid #d2d2d7;
    text-align: center;
    color: var(--apple-gray);
}

/* Responsive */
@media (max-width: 768px) {
    .nav-menu {
